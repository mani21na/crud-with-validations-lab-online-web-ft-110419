<h1>Songs</h1>
<!--    <%= link_to "Delete", song, method: :delete, data: { confirm: "Are you sure?" } %> -->


<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Released</th>
            <th>Release_year</th>
            <th>artist_name</th>
            <th>genre</th>
        </tr>
    </thead>
    <tbody>
        <% @songs.each do |song| %>
            <tr>
                <td><%= song.title %></td>
                <td><%= song.released %></td>
                <td><%= song.release_year %></td>
                <td><%= song.artist_name %></td>
                <td><%= song.genre %></td>
            </tr>
            <%= link_to "Delete", song, method: :delete, data: { confirm: "Are you sure?" } %>
        <% end %>
    </tbody>
</table>
