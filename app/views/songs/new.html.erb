<h1>Add Song</h1>
<%= form_for @song do |f| %>
  <% if @song.errors.any? %>
    <div id="error_explanation">
      <h3>There were some errors:</h3>
      <ul>
        <% @post.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <%= f.text_field :title %>
  <%= f.text_field :released %>
  <%= f.text_filed :release_year %>
  <%= f.text_field :artist_name%>
  <%= f.text_field :genre %>

  <%= f.submit %>
<% end %>
